<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.5.2.14234" newVersion="1.5.2.14234" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <container name="Unity">
      <register name="video" type="ElysiumMagtheridon.Models.IProgressionItem, ElysiumMagtheridon" mapTo="ElysiumMagtheridon.Models.ProgressionVideo, ElysiumMagtheridon">
        <constructor>
          <param name="videoType" value="MUH VID" />
        </constructor>
      </register>

      <register type="ElysiumMagtheridon.Controllers.ProgressionController, ElysiumMagtheridon">
        <constructor>
          <param name="vids">
            <array>
              <dependency name="video" />
            </array>
          </param>
        </constructor>
      </register>
    </container>
  </unity>
</configuration>
